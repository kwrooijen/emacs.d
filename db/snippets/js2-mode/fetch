# -*- mode: snippet -*-
# name: fetchg
# key: fetchg
# --
fetch("/$1", {
      method: "GET",
      headers: {
        "Content-Type": "application/json",
        "Accept": "application/json"
      }
    })
      .then(response => {
        if (!response.ok) {
          throw new Error(`HTTP error! Status: ${response.status}`);
        }
        $0return response.json();
      })
      .catch(error => {
        console.error("Error sending request:", error);
      });